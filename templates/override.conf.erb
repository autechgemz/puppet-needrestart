# Managed by Puppet
<% @content.each do |keys, values| -%>
<% if values.is_a?(String) -%>
$nrconf{<%= keys %>} = '<%= values -%>';
<% end -%>
<% if values.is_a?(Integer) -%>
$nrconf{<%= keys %>} = <%= values -%>;
<% end -%>
<% if values.is_a?(Array) -%>
$nrconf{<%= keys %>} = [
<% values.each do |value| -%>
    <%= value %>,
<% end -%>
];
<% end -%>
<% if values.is_a?(Hash) -%>
$nrconf{<%= keys %>} = {
<% values.each do |key, value| -%>
<% if value.is_a?(String) -%>
    <%= key %> => '<%= value %>',
<% elsif value.is_a?(Integer) -%>
    <%= key %> => <%= value %>,
<% elsif value.is_a?(Array) -%>
    <%= key %> = [
<% value.each do |v| %>
    <%= v %>,
<% end %>
]
<% end -%>
<% end -%>
};
<% end -%>
<% end -%>
